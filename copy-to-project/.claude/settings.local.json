{
  "plansDirectory": "./docs/plans",
  "permissions": {
    "wildcards": [
      "Bash(ruff*)",
      "Bash(pytest*)",
      "Bash(python -m pytest*)",
      "Bash(mypy*)",
      "Bash(make check*)",
      "Bash(make test*)"
    ],
    "defaultMode": "acceptEdits"
  },
  "env": {
    "ENABLE_TOOL_SEARCH": "auto:5"
  },
  "skillHotReload": true,
  "forkedContextEnabled": true
}
